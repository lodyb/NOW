{
  "apps": [
    {
      "name": "now-discord-bot",
      "script": "dist/index.js",
      "watch": false,
      "env": {
        "NODE_ENV": "production"
      },
      "max_memory_restart": "500M",
      "restart_delay": 3000,
      "exp_backoff_restart_delay": 100,
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "merge_logs": true
    }
  ]
}