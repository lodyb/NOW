{
  "name": "now-discord-bot",
  "version": "0.1.0",
  "description": "Discord bot for media collection and quiz games",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "pm2:start": "pm2 start dist/index.js --name now-bot",
    "pm2:ecosystem": "pm2 start ecosystem.config.json",
    "pm2:stop": "pm2 stop now-discord-bot",
    "pm2:restart": "pm2 restart now-discord-bot",
    "pm2:logs": "pm2 logs now-discord-bot",
    "pm2:dev:start": "pm2 start ecosystem.config.json --only now-discord-bot-dev",
    "pm2:dev:stop": "pm2 stop now-discord-bot-dev",
    "pm2:dev:restart": "pm2 restart now-discord-bot-dev",
    "pm2:dev:logs": "pm2 logs now-discord-bot-dev",
    "pm2:dev:monitor": "pm2 monit",
    "production": "npm run build && npm run pm2:ecosystem"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.9.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "fluent-ffmpeg": "^2.1.2",
    "fuse.js": "^6.6.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "say": "^0.16.0",
    "sqlite3": "^5.1.6",
    "tweetnacl": "^1.0.3",
    "vue": "^3.5.13",
    "winston": "^3.17.0",
    "youtube-dl-exec": "^3.0.21",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.1",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jest": "^29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/sqlite3": "^3.1.8",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
