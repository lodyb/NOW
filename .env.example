# Discord Bot Token
DISCORD_TOKEN="your_discord_token"

# Discord OAuth2 credentials (create these in Discord Developer Portal)
DISCORD_CLIENT_ID=your_discord_app_client_id
DISCORD_CLIENT_SECRET=your_discord_app_client_secret
DISCORD_CALLBACK_URL=http://localhost:3000/auth/discord/callback

# Session security
SESSION_SECRET=a_strong_random_secret_key

# Environment (development or production)
NODE_ENV=development

# Web server port
PORT=3000

# Music library path for radio
MUSIC_LIBRARY_PATH=/media/enka/music

# LLM
LLM_MAX_TOKENS='2048'
LLM_TEMPERATURE='0.7'
LLM_GPU_LAYERS='35'
LLM_CONTEXT_SIZE='4096'
LLM_BATCH_SIZE='512'
LLM_TIMEOUT='60000'

# Ollama Settings
OLLAMA_URL=http://127.0.0.1:11434
LLM_MODEL_NAME=deepseek-r1-llama-8b